{
    "name": "dorcas/modules-ops-php",
    "description": "Sample Dorcas Hub Module",
    "type": "library",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Odumosu Oluwashina",
            "email": "oluwashinaodumosu@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "aws/aws-sdk-php": "^3.74",
        "guzzlehttp/guzzle": "^6.3",
        "laravel/framework": "^5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Dorcas\\ModulesOps\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dorcas\\ModulesOps\\ModulesOpsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
